package day3;

public class test_sl {
	 public static void main(String[] args) {
	        singly list = new SinglyLinkedList();

	        list.insertAtBeginning(10); // Insert 10 at the beginning
	        list.insertAtBeginning(20); // Insert 20 at the beginning
	        list.insertAtBeginning(30); // Insert 30 at the beginning

	        System.out.print("List after insertion at beginning: ");
	        list.display();

	        list.insertAtPosition(25, 1); // Insert 25 at position 1
	        System.out.print("List after insertion at position 1: ");
	        list.display();

	        list.insertAtPosition(5, 0); // Insert 5 at position 0 (beginning)
	        System.out.print("List after insertion at position 0: ");
	        list.display();
	    }

}
